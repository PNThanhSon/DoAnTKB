<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.TimeTableController">
   <Label prefHeight="73.0" prefWidth="282.0" text="XEM CÁC THỜI KHÓA BIỂU">
      <font>
         <Font name="System Bold" size="22.0" />
      </font>
   </Label>
   <Button fx:id="btnTKBToanTruong" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleTKBToanTruong" prefHeight="40.0" prefWidth="110.0" text="Toàn trường" textAlignment="CENTER">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Button>
   <Button fx:id="btnTKBLopCN" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleTKBLopCN" prefHeight="40.0" prefWidth="110.0" text="Lớp chủ nhiệm" textAlignment="CENTER">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Button>
   <Button fx:id="btnTKBToCM" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleTKBToCM" prefHeight="40.0" prefWidth="110.0" text="Tổ chuyên môn" textAlignment="CENTER">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Button>
   <Button fx:id="btnTKBCaNhan" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleTKBCaNhan" prefHeight="40.0" prefWidth="110.0" text="Cá nhân" textAlignment="CENTER">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Button>
</VBox>
